{
  "name": "ambrosia-bakery-sf",
  "version": "1.0.0",
  "description": "web page for Ambrosia Bakery in San Francisco",
  "main": "./app.js",
  "homepage": "http://ambrosiabakerysf.com",
  "author": "Josh Kang",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "build:server": "NODE_ENV=production webpack --config ./webpack.server.config.js --progress --colors",
    "start": "node app.js",
    "start:prod": "NODE_ENV=production node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kngroo/ambrosia.git"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "es2015",
    "javascript"
  ],
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-images": "^0.5.13",
    "react-router": "^2.8.1",
    "react-scroll": "^1.7.3",
    "skeleton-css": "^2.0.4",
    "skeleton-css-webpack": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
